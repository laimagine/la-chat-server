{
  "name": "la-imagine-chat",
  "version": "1.0.0",
  "author": "LA Chat by LA Imagine",
  "description": "LA Chat server is a nodejs backend server for LA Chat app by LA Imagine.",
  "homepage": "https://www.laimagine.com/",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
